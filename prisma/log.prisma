datasource db {
  provider = "postgresql"
  url      = env("LOG_DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
  output = "./generate/client2"
}

model Log {
  id        Int      @id @default(autoincrement())
  endpoint  String
  body      String
  status    Int
  userID    Int?
  message   String?
  createdAt DateTime @default(now())
}
